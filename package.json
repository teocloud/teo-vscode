{
  "name": "teo-vscode",
  "displayName": "Teo",
  "description": "Teo language support",
  "version": "0.0.16",
  "publisher": "yeannylam",
  "repository": {
    "type": "git",
    "url": "https://github.com/victorteokw/teo-vscode.git"
  },
  "engines": {
    "vscode": "^1.82.0"
  },
  "scripts": {
    "build": "tsc"
  },
  "main": "./dist/src/extension.js",
  "activationEvents": [
		"onLanguage:teo"
	],
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "teo",
        "aliases": [
          "Teo",
          "teo",
          "TEO"
        ],
        "extensions": [
          ".teo"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "teo",
        "scopeName": "source.teo",
        "path": "./syntaxes/teo.tmLanguage.json"
      }
    ]
  },
  "dependencies": {
    "@teocloud/teo-language-server": "^0.0.12",
    "vscode-languageclient": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/vscode": "^1.82.0",
    "@vscode/vsce": "^2.21.1",
    "typescript": "^5.2.2"
  }
}
