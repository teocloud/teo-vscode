{
  "name": "teo-vscode",
  "displayName": "teo-vscode",
  "description": "This extension provides enhanced Teo language and code intellisense support for Visual Studio Code.",
  "version": "0.0.4",
  "publisher": "yeannylam",
  "repository": {
    "type": "git",
    "url": "https://github.com/victorteokw/teo-vscode.git"
  },
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "teo",
      "aliases": ["Teo", "teo", "TEO"],
      "extensions": [".teo"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "teo",
      "scopeName": "source.teo",
      "path": "./syntaxes/teo.tmLanguage.json"
    }]
  }
}
