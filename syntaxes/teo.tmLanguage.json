{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Teo",
	"scopeName": "source.teo",
	"fileTypes": ["teo"],
	"patterns": [
    {
			"include": "#tripleSlashComment"
		},
		{
			"include": "#doubleSlashComment"
		},
    {
      "include": "#identifierImport"
    },
		{
			"include": "#fileImport"
		},
    {
      "include": "#importKeyword"
    },
    {
      "include": "#constantDeclaration"
    },
    {
      "include": "#configBlock"
    },
    {
      "include": "#namedConfigBlock"
    },
    {
      "include": "#datasetBlock"
    },
    {
      "include": "#configKeyword"
    },
		{
			"include": "#modelBlock"
		},
    {
      "include": "#modelKeyword"
    },
		{
      "include": "#enumBlock"
    },
    {
      "include": "#enumKeyword"
    },
    {
      "include": "#interfaceDeclaration"
    },    
    {
      "include": "#middlewareDeclaration"
    },
    {
      "include": "#actionGroupDeclaration"
    },
    {
      "include": "#actionDeclaration"
    },
    {
      "include": "#declareKeyword"
    },
		{
			"include": "#strings"
		},
    {
      "include": "#regexs"
    },
    {
      "include": "#attributeWithArguments"
    },
    {
      "include": "#attribute"
    },
    {
      "include": "#pipeline"
    }
	],
	"repository": {
    "middlewareDeclaration": {
      "name": "meta.middlewareDeclaration.teo",
      "match": "(declare)\\s+(middleware)\\s+([A-Za-z_][A-Za-z0-9_]*)",
      "captures": {
        "1": {
          "name": "storage.modifier.keyword.teo"
        },
        "2": {
          "name": "storage.modifier.keyword.teo"
        },
        "3": {
          "name": "entity.name.type.model.teo"
        }
      }
    },
    "actionGroupDeclaration": {
      "name": "meta.actionGroupDeclaration.teo",
      "match": "(declare)\\s+(action)\\s+(group)\\s+([A-Za-z_][A-Za-z0-9_]*)",
      "captures": {
        "1": {
          "name": "storage.modifier.keyword.teo"
        },
        "2": {
          "name": "storage.modifier.keyword.teo"
        },
        "3": {
          "name": "storage.modifier.keyword.teo"
        },
        "4": {
          "name": "entity.name.type.model.teo"
        }
      }
    },
    "actionDeclaration": {
      "name": "meta.actionDeclaration.teo",
      "begin": "(declare)\\s+(action)\\s+([A-Za-z_][A-Za-z0-9_]*)",
      "beginCaptures": {
        "1": {
          "name": "storage.modifier.keyword.teo"
        },
        "2": {
          "name": "storage.modifier.keyword.teo"
        },
        "3": {
          "name": "entity.name.type.model.teo"
        }
      },
      "patterns": [
        {
          "include": "#expressionPunctuations"
        },
        {
          "include": "#punctuation-colon"
        },
        {
          "include": "#punctuation-comma"
        },
        {
          "include": "#parenOpen"
        },
        {
          "include": "#parenClose"
        },
        {
          "include": "typeParameters"
        },
        {
          "match": "[A-Za-z_][A-Za-z0-9_]*",
          "captures": {
            "0": {
              "name": "entity.other.inherited-class.teo"
            }
          }
        }
      ],
      "end": "\n"
    },
    "interfaceHeritage": {
      "name": "meta.interfaceHeritage.teo",
      "begin": "\\s*(extends)\\s*",
      "beginCaptures": {
        "1": { 
          "name": "storage.modifier.teo" 
        }
      },
      "end": "(?=\\{)",
      "patterns": [
        {
          "include": "#doubleSlashComment"
        },
        {
          "include": "#tripleSlashComment"
        },
        {
          "match": "[A-Za-z_][A-Za-z0-9_]*",
          "captures": {
            "0": {
              "name": "entity.other.inherited-class.teo"
            }
          }
        },
        {
          "include": "#interfaceHeritage"
        },
        {
          "include": "#typeParameters"
        },
        {
          "include": "#expressionPunctuations"
        }
      ]
    },
    "typeParameters": {
      "name": "meta.type.parameters.teo",
      "begin": "(<)",
      "beginCaptures": {
        "1": {
          "name": "punctuation.definition.typeparameters.begin.teo"
        }
      },
      "end": "(>)",
      "endCaptures": {
        "1": {
          "name": "punctuation.definition.typeparameters.end.teo"
        }
      },
      "patterns": [
        {
          "include": "#doubleSlashComment"
        },
        {
          "include": "#tripleSlashComment"
        },
        {
          "include": "#punctuation-comma"
        },
        {
          "match": "[A-Za-z_][A-Za-z0-9_]*",
          "captures": {
            "0": {
              "name": "entity.name.type.interface.teo"
            }
          }
        }
      ]
    },
    "expressionPunctuations": {
      "patterns": [
        {
          "include": "#punctuation-comma"
        },
        {
          "include": "#punctuation-accessor"
        }
      ]
    },
    "punctuation-comma": {
      "name": "punctuation.separator.comma.teo",
      "match": ","
    },
    "punctuation-colon": {
      "name": "punctuation.terminator.statement.teo",
      "match": ":"
    },
    "punctuation-semicolon": {
      "name": "punctuation.terminator.statement.teo",
      "match": ";"
    },
    "punctuation-accessor": {
      "match": "(?:(\\.)|(\\?\\.(?!\\s*[[:digit:]])))",
      "captures": {
        "1": { "name": "punctuation.accessor.teo" },
        "2": { "name": "punctuation.accessor.optional.teo" }
      }
    },
    "interfaceDeclaration": {
      "name": "meta.interface.teo",
      "begin": "(interface)\\s+(\\w+)",
      "beginCaptures": {
        "1": {
          "name": "storage.modifier.teo"
        },
        "2": {
          "name": "entity.name.type.interface.teo"
        }
      },
      "patterns": [
        {
          "include": "#doubleSlashComment"
        },
        {
          "include": "#tripleSlashComment"
        },
        {
          "include": "#interfaceHeritage"
        },
        {
          "match": "([A-Za-z_][A-Za-z0-9_]*)",
          "captures": {
            "1": {
              "name": "entity.name.type.interface.teo"
            }
          }
        },
        {
          "include": "#typeParameters"
        },
        {
          "include": "#interfaceBody"
        }
      ],
      "end": "(?<=\\})"
    },
    "interfaceBody": {
      "begin": "\\{",
      "beginCaptures": {
        "0": { "name": "punctuation.definition.block.teo" }
      },
      "end": "\\}",
      "endCaptures": {
        "0": { "name": "punctuation.definition.block.teo" }
      },
      "patterns": [
        {
          "include": "#doubleSlashComment"
        },
        {
          "include": "#tripleSlashComment"
        },
        {
					"include": "#fieldDefinition"
				}
      ]
    },
    "constantDeclaration": {
      "name": "variable.name.teo",
      "begin": "(let)\\s+(\\w+)\\s+(=)\\s+",
      "beginCaptures": {
        "1": {
          "name": "keyword.control.teo"
        },
        "2": {
          "name": "variable.name.teo"
        },
        "3": {
          "name": "punctuation.definition.tag.teo"
        }
      },
      "patterns": [
        {
          "include": "#value"
        }
      ],
      "end": "\r\n|\r|\n"
    },
		"fileImport": {
			"name": "storage.modifier.import.teo",
			"patterns": [
				{
					"match": "(import)\\s+",
					"captures": {
						"1": {
							"name": "keyword.control.teo"
						}
					}
				},
				{
					"include": "strings"
				}
			]
		},
    "identifierImport": {
			"name": "storage.modifier.import.teo",
      "begin": "(import)\\s+({)",
      "beginCaptures": {
        "1": {
          "name": "keyword.control.teo"
        },
        "2": {
          "name": "punctuation.definition.tag.teo"
        }
      },
      "patterns": [
        {
          "match": "\\s*(\\w+)\\s*(,?)\\s*",
          "captures": {
            "1": {
              "name": "variable.other.assignment.teo"
            },
            "2": {
              "name": "punctuation.definition.tag.teo"
            }
          }
        }
      ],
      "end": "(})\\s+(from)\\s*",
      "endCaptures": {
        "1": {
          "name": "punctuation.definition.tag.teo"
        },
        "2": {
          "name": "keyword.control.teo"
        }
      }
    },
    "declareKeyword": {
      "name": "storage.modifier.keyword.teo",
      "patterns": [
        {
          "match": "(declare)",
          "captures": {
            "1": {
              "name": "storage.modifier.keyword.teo"
            }
          }
        }
      ]
    },
    "actionKeyword": {
      "name": "storage.modifier.keyword.teo",
      "patterns": [
        {
          "match": "(action)",
          "captures": {
            "1": {
              "name": "storage.modifier.keyword.teo"
            }
          }
        }
      ]
    },
    "middlewareKeyword": {
      "name": "storage.modifier.keyword.teo",
      "patterns": [
        {
          "match": "(middleware)",
          "captures": {
            "1": {
              "name": "storage.modifier.keyword.teo"
            }
          }
        }
      ]
    },
    "usingKeyword": {
      "name": "storage.modifier.keyword.teo",
      "patterns": [
        {
          "match": "(using)",
          "captures": {
            "1": {
              "name": "storage.modifier.keyword.teo"
            }
          }
        }
      ]
    },
    "formKeyword": {
      "name": "storage.modifier.keyword.teo",
      "patterns": [
        {
          "match": "(form)",
          "captures": {
            "1": {
              "name": "storage.modifier.keyword.teo"
            }
          }
        }
      ]
    },
    "jsonKeyword": {
      "name": "storage.modifier.keyword.teo",
      "patterns": [
        {
          "match": "(json)",
          "captures": {
            "1": {
              "name": "storage.modifier.keyword.teo"
            }
          }
        }
      ]
    },
    "enumKeyword": {
      "name": "storage.modifier.keyword.teo",
      "patterns": [
        {
          "match": "(enum)",
          "captures": {
            "1": {
              "name": "storage.modifier.keyword.teo"
            }
          }
        }
      ]
    },
    "importKeyword": {
      "name": "storage.modifier.config.teo",
      "patterns": [
        {
          "match": "(import)",
          "captures": {
            "1": {
              "name": "storage.type.config.teo"
            }
          }
        }
      ]
    },
    "configKeyword": {
      "name": "storage.modifier.config.teo",
      "patterns": [
        {
          "match": "(connector|client|entity|server|autoseed dataset|autoseed|dataset|group|record)",
          "captures": {
            "1": {
              "name": "storage.type.config.teo"
            }
          }
        }
      ]
    },
    "modelKeyword": {
      "name": "storage.modifier.keyword.teo",
      "patterns": [
        {
          "match": "(model)",
          "captures": {
            "1": {
              "name": "storage.type.model.teo"
            }
          }
        }
      ]
    },
    "configBlock": {
      "begin": "^\\s*(entity|client|connector|server)\\s+({)",
      "name": "source.teo.embedded.config",
      "beginCaptures": {
        "1": {
          "name": "storage.type.config.teo"
        },
        "2": {
          "name": "punctuation.definition.tag.teo"
        }
      },
      "patterns": [
        {
          "include": "#tripleSlashComment"
        },
        {
          "include": "#doubleSlashComment"
        },
        {
          "include": "#configItem"
        }
      ],
      "end": "\\s*\\}",
      "endCaptures": {
        "1": {
          "name": "punctuation.definition.tag.teo"
        }
      }
    },
    "datasetRecordItem": {
      "begin": "^\\s*(record)\\s+([A-Za-z][\\w]*)\\s+({)",
      "name": "source.teo.embedded.config",
      "beginCaptures": {
        "1": {
          "name": "storage.type.config.teo"
        },
        "2": {
          "name": "entity.name.type.config.teo"
        },
        "3": {
          "name": "punctuation.definition.tag.teo"
        }
      },
      "patterns": [
        {
          "include": "#tripleSlashComment"
        },
        {
          "include": "#doubleSlashComment"
        },
        {
          "include": "#value"
        },
        {
          "include": "#colon"
        }
      ],
      "end": "\\s*\\}",
      "endCaptures": {
        "1": {
          "name": "punctuation.definition.tag.teo"
        }
      }
    },
    "datasetGroupItem": {
      "begin": "^\\s*(group)\\s+([A-Za-z][\\w]*)\\s+({)",
      "name": "source.teo.embedded.config",
      "beginCaptures": {
        "1": {
          "name": "storage.type.config.teo"
        },
        "2": {
          "name": "entity.name.type.config.teo"
        },
        "3": {
          "name": "punctuation.definition.tag.teo"
        }
      },
      "patterns": [
        {
          "include": "#tripleSlashComment"
        },
        {
          "include": "#doubleSlashComment"
        },
        {
          "include": "#datasetRecordItem"
        }
      ],
      "end": "\\s*\\}",
      "endCaptures": {
        "1": {
          "name": "punctuation.definition.tag.teo"
        }
      }
    },
    "datasetBlock": {
      "begin": "^\\s*(autoseed dataset|autoseed|dataset)\\s+([A-Za-z][\\w]*)\\s+({)",
      "name": "source.teo.embedded.config",
      "beginCaptures": {
        "1": {
          "name": "storage.type.config.teo"
        },
        "2": {
          "name": "entity.name.type.config.teo"
        },
        "3": {
          "name": "punctuation.definition.tag.teo"
        }
      },
      "patterns": [
        {
          "include": "#tripleSlashComment"
        },
        {
          "include": "#doubleSlashComment"
        },
        {
          "include": "#datasetGroupItem"
        }
      ],
      "end": "\\s*\\}",
      "endCaptures": {
        "1": {
          "name": "punctuation.definition.tag.teo"
        }
      }
    },
    "namedConfigBlock": {
      "begin": "^\\s*(entity|client|connector|server)\\s+([A-Za-z][\\w]*)\\s+({)",
      "name": "source.teo.embedded.config",
      "beginCaptures": {
        "1": {
          "name": "storage.type.config.teo"
        },
        "2": {
          "name": "entity.name.type.config.teo"
        },
        "3": {
          "name": "punctuation.definition.tag.teo"
        }
      },
      "patterns": [
        {
          "include": "#tripleSlashComment"
        },
        {
          "include": "#doubleSlashComment"
        },
        {
          "include": "#configItem"
        }
      ],
      "end": "\\s*\\}",
      "endCaptures": {
        "1": {
          "name": "punctuation.definition.tag.teo"
        }
      }
    },
    "configItem": {
      "name": "source.teo.embedded.source",
      "patterns": [
        {
          "begin": "^\\s*(\\w+)\\s+",
          "beginCaptures": {
            "1": {
              "name": "keyword.control.teo"
            }
          },
          "patterns": [
            {
              "include": "#value"
            },
            {
              "include": "#doubleSlashCommentInline"
            }
          ],
          "end": "\\n"
        }
      ]
    },
		"modelBlock": {
			"begin": "^\\s*(model)\\s+([A-Z][A-Za-z]*[\\w]*)\\s*({)",
			"beginCaptures": {
				"1": {
          "name": "storage.type.model.teo"
        },
        "2": {
          "name": "entity.name.type.model.teo"
        },
        "3": {
          "name": "punctuation.definition.tag.teo"
        }
			},
			"patterns": [
				{
					"include": "#tripleSlashComment"
				},
				{
					"include": "#doubleSlashComment"
				},
				{
					"include": "#fieldDefinition"
				}
			],
			"end": "\\s*\\}",
			"endCaptures": {
        "1": {
          "name": "punctuation.definition.tag.teo"
        }
      }
		},
		"fieldDefinition": {
      "name": "scalar.field",
      "patterns": [
        {
          "include": "#attributeWithArguments"
        },
        {
          "include": "#attribute"
        },
        {
          "match": "^\\s*(\\w+)(\\s*:)\\s*((?!(?:Int|String|DateTime|Date|Bytes|Decimal|Float|Bool|ObjectId)\\b)\\b\\w+)?(Int|String|DateTime|Date|Bytes|Decimal|Float|Bool|ObjectId)?(\\[\\])?(\\?)?(\\!)?",
          "captures": {
            "1": {
              "name": "variable.other.assignment.teo"
            },
            "2": {
              "name": "punctuation.definition.colon.teo"
            },
            "3": {
              "name": "variable.language.relations.teo"
            },
            "4": {
              "name": "support.type.primitive.teo"
            },
            "5": {
              "name": "keyword.operator.list_type.teo"
            },
            "6": {
              "name": "keyword.operator.optional_type.teo"
            },
            "7": {
              "name": "invalid.illegal.required_type.teo"
            }
          }
        }
      ]
    },
		"enumBlock": {
			"begin": "^\\s*(interface)?\\s*(enum)\\s+([A-Z][A-Za-z]*[\\w]*)\\s*({)",
      "name": "source.teo.embedded.source",
      "beginCaptures": {
        "1": {
          "name": "storage.type.modifier.teo"
        },
        "2": {
          "name": "storage.type.enum.teo"
        },
        "3": {
          "name": "entity.name.type.enum.teo"
        },
        "4": {
          "name": "punctuation.definition.tag.teo"
        }
      },
      "patterns": [
				{
					"include": "#tripleSlashComment"
				},
				{
					"include": "#doubleSlashComment"
				},
        {
          "include": "#enum_value_definition"
        }
      ],
      "end": "(\\s*\\})",
      "endCaptures": {
        "1": {
          "name": "punctuation.definition.tag.teo"
        }
      }
    },
		"doubleSlashComment": {
      "begin": "//",
      "end": "$\\n?",
      "name": "comment.teo.normal"
    },
    "doubleSlashCommentInline": {
      "begin": "//",
      "end": "$(?=\\n)",
      "name": "comment.teo.normal.inline"
    },
		"tripleSlashComment": {
      "begin": "///",
      "end": "$\\n?",
      "name": "comment.teo.doc"
		},
    "null": {
      "match": "\\b(null)\\b",
      "name": "constant.language.null.teo"
    },
		"bools": {
      "match": "\\b(true|false)\\b",
      "name": "constant.language.boolean.teo"
    },
    "numbers": {
      "match": "((0(x|X)[0-9a-fA-F]*)|(\\+|-)?\\b(([0-9]+\\.?[0-9]*)|(\\.[0-9]+))((e|E)(\\+|-)?[0-9]+)?)([LlFfUuDdg]|UL|ul)?\\b",
      "name": "constant.numeric.teo"
    },
		"strings": {
			"name": "string.quoted.double.teo",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.teo",
					"match": "\\\\."
				}
			]
		},
    "regexs": {
      "name": "string.regexp.teo",
      "begin": "/(?!/)",
      "end": "/",
      "patterns": [
        {
					"name": "constant.character.escape.teo",
					"match": "\\\\."
				}
      ]
    },
    "enumValues": {
      "name": "variable.other.enummember.teo",
      "match": "\\.[^\\d\\W]\\w*"
    },
		"literal": {
      "name": "source.teo.literal",
      "patterns": [
        {
          "include": "#null"
        },
        {
          "include": "#bools"
        },
        {
          "include": "#numbers"
        },
        {
          "include": "#strings"
        },
        {
          "include": "#regexs"
        }
      ]
    },
		"attribute": {
      "name": "source.teo.attribute",
      "match": "(@@?[\\w\\.]+)",
      "captures": {
        "1": {
          "name": "entity.name.function.attribute.teo"
        }
      }
    },
		"attributeWithArguments": {
      "name": "source.teo.attribute.withWrguments",
      "begin": "(@@?[\\w\\.]+)(\\()",
      "beginCaptures": {
        "1": {
          "name": "entity.name.function.attribute.teo"
        },
        "2": {
          "name": "punctuation.definition.tag.teo"
        }
      },
      "patterns": [
        {
          "include": "#namedArgument"
        },
        {
          "include": "#value"
        }
      ],
      "end": "(\\))",
      "endCaptures": {
        "1": {
          "name": "punctuation.definition.tag.teo"
        }
      }
    },
		"builtinTypes": {
			"name": "storage.type.teo",
			"match": "\\b(String|Int|Int32|Int64|Float|Float32|Float64|Decimal|Bool|Date|DateTime|Bytes|ObjectId)\\b"
		},
		"namedArgument": {
      "name": "source.teo.namedArgument",
      "patterns": [
        {
          "include": "#mapKey"
        },
        {
          "include": "#value"
        }
      ]
    },
		"enumValueDefinition": {
      "patterns": [
        {
          "match": "^\\s*(\\w+)\\s*",
          "captures": {
            "1": {
              "name": "variable.other.assignment.teo"
            }
          }
        },
        {
          "include": "#attributeWithArguments"
        },
        {
          "include": "#attribute"
        }
      ]
    },
		"mapKey": {
      "name": "source.teo.key",
      "patterns": [
        {
          "match": "(\\w+)\\s*(:)\\s*",
          "captures": {
            "1": {
